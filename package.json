  "scripts": {
    "postinstall": "yarnw-sync-deps && yarn-deduplicate",
    "bootstrap": "lerna bootstrap --use-workspaces",
    "build": "set NODE_ENV=publish&& lerna exec \"babel --root-mode upward src -d lib --ignore **/*.stories.js,**/*.spec.js,**/*.stories.mdx,**/*.test.js --copy-files --delete-dir-on-start\"",
    "storybook": "set NODE_ENV=development&& start-storybook",
    "upload": "yarn build && lerna publish",
    "deploy": "yarn build && build-storybook --docs",
    "test": "jest --collectCoverage --silent --runInBand",
    "deploy-storybook": "storybook-to-ghpages -- --out=.out --docs",
    "e2e:develop": "yarn workspace example develop",
    "e2e:stage": "yarn workspace example serve -p 8000",
    "e2e:watch": "yarn run cypress open",
    "e2e:test": "yarn run cypress run"
  },
  "workspaces": [
    "example",
    "gatsby-theme-q3",
    "packages/*"

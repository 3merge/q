{
  "name": "q3-client",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "postinstall": "yarnw-sync-deps && yarn-deduplicate",
    "bootstrap": "lerna bootstrap --use-workspaces",
    "build": "set NODE_ENV=publish&& lerna exec \"babel --root-mode upward src -d lib --ignore **/*.stories.js,**/*.spec.js --copy-files\"",
    "storybook": "set NODE_ENV=development&& start-storybook",
    "upload": "yarn build && lerna publish",
    "deploy": "yarn build && build-storybook --docs",
    "test": "jest --collectCoverage --silent --runInBand",
    "deploy-storybook": "storybook-to-ghpages -- --out=.out"
  },
  "workspaces": [
    "packages/*",
    "integration-testing"
  ],
  "resolutions": {
    "acorn": "^7.1.1",
    "kind-of": "^6.0.3"
  },

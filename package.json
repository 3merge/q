{
  "name": "q3-client",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "yarn": "^1.17.3",
    "node": "^12.9.0"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap --use-workspaces",
    "build": "set NODE_ENV=publish&& lerna exec \"babel --root-mode upward src -d lib --ignore **/*.stories.js,**/*.spec.js --copy-files\"",
    "lint": "eslint . --ext .js --ext .jsx --ignore-pattern lib --ignore-pattern node_modules",
    "test": "jest --runInBand --collectCoverage --json --outputFile=.storybook/jestResults.json",
    "storybook": "set NODE_ENV=development&& start-storybook",
    "upload": "yarn build && lerna publish",
    "docs": "storybook-to-ghpages"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/preset-env": "^7.5.5",
    "axios-mock-adapter": "^1.17.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-node-3merge": "^1.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-react-3merge": "^1.0.1",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-import-resolver-lerna": "^1.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-module-resolver": "^0.15.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "lerna": "^3.18.1",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.31",
    "@material-ui/styles": "^4.5.0",
    "@reach/router": "^1.2.1",
    "@storybook/addon-a11y": "^5.2.4",
    "@storybook/addon-docs": "^5.2.5",
    "@storybook/addon-info": "^5.2.5",
    "@storybook/addon-jest": "^5.2.4",
    "@storybook/addon-viewport": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "@storybook/storybook-deployer": "^2.8.1",
    "@svgr/webpack": "^4.3.3",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.0-beta.1",
    "formik": "^2.0.4",
    "i18next": "^19.0.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-i18next": "^10.13.1",
    "storybook-readme": "^5.0.8"
  }
}